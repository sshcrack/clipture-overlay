    "minimist":"1.2.6",
    "colors": "^1.4.0",
    "fs": "^0.0.1-security",
    "node-addon-api": "^4.3.0",
    "path": "^0.12.7",
    "shelljs": "^0.8.5",
    "tar": "6.1.11"
  },
